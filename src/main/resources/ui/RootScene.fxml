<?import javafx.scene.layout.*?>

<AnchorPane fx:id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.RootController">
    <GridPane maxHeight="200.0" maxWidth="400.0" minHeight="200.0" minWidth="400.0" prefHeight="200.0" prefWidth="400.0"
              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <cursor>
            <Cursor fx:constant="HAND"/>
        </cursor>
        <AnchorPane>
            <Button alignment="BOTTOM_CENTER" contentDisplay="TOP" graphicTextGap="10.0" mnemonicParsing="false"
                    onAction="#gotoCasl2EditMode" prefHeight="200.0" prefWidth="207.0"
                    text="アセンブリプログラミング&#10;&#10;" wrapText="true" AnchorPane.bottomAnchor="0.0"
                    AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
                    GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                <graphic>
                    <ImageView pickOnBounds="true" preserveRatio="true">
                        <Image url="@../1444519438_document_text.png"/>
                    </ImageView>
                </graphic>
            </Button>
        </AnchorPane>
        <AnchorPane GridPane.columnIndex="1">
            <Button alignment="BOTTOM_CENTER" contentDisplay="TOP" graphicTextGap="10.0" mnemonicParsing="false"
                    onAction="#gotoComet2EditMode" prefHeight="200.0" prefWidth="200.0"
                    text="機械語プログラミング&#10;            " AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" GridPane.columnIndex="2">
                <graphic>
                    <ImageView pickOnBounds="true" preserveRatio="true">
                        <Image url="@../1444519116_Device_and_Hardware.png"/>
                    </ImageView>
                </graphic>
            </Button>
        </AnchorPane>
    </GridPane>
</AnchorPane>
